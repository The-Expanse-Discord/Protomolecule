{
	"name": "@the-expanse-discord/protomolecule",
	"description": "General purpose bot for The Expanse Discord server.",
	"main": "bin/index.js",
	"version": "3.1.8",
	"license": "MIT",
	"author": "kata-gatame <nathan@kata.codes>",
	"contributors": [
		"John Daniels (https://github.com/johndaniels)",
		"acdcfanbill (https://github.com/acdcfanbill)"
	],
	"repository": "https://github.com/The-Expanse-Discord/Protomolecule.git",
	"homepage": "https://github.com/The-Expanse-Discord/Protomolecule#readme",
	"bugs": "https://github.com/The-Expanse-Discord/Protomolecule/issues",
	"bin": {
		"protomolecule": "bin/protomolecule.js"
	},
	"scripts": {
		"code:lint": "eslint './src/**/*.ts'",
		"code:build": "tsc -p ./tsconfig.json",
		"docs:build": "typedoc",
		"db:seed": "ts-node ./src/Infrastructure/Database/Inserts/index.ts",
		"dev": "ts-node ./src/cmd.ts --config config.json5",
		"prepare": "npm run code:build"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"cheerio": "^1.0.0-rc.5",
		"discord.js": "^12.5.1",
		"json5": "^2.2.0",
		"yargs": "^16.2.0"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.24",
		"@types/jest": "^26.0.20",
		"@types/json5": "^0.0.30",
		"@types/node": "^14.14.32",
		"@types/yargs": "^16.0.0",
		"@typescript-eslint/eslint-plugin": "^4.16.1",
		"@typescript-eslint/parser": "^4.16.1",
		"eslint": "^7.21.0",
		"jest": "^26.6.3",
		"ts-jest": "^26.5.3",
		"ts-node": "^9.1.1",
		"typedoc": "^0.20.30",
		"typescript": "^4.2.3"
	},
	"files": [
		"dist/**",
		"bin/*"
	]
}
