		"reflect-metadata": "^0.1.13",
		"yargs": "^16.0.3"
	},
	"devDependencies": {
		"@kata-codes/eslint-config": "github:kata-codes/eslint-config",
		"@types/cheerio": "^0.22.22",
		"@types/jest": "^24.0.23",
		"@types/node": "^14.11.8",
		"@types/pg": "^7.14.5",
		"@types/yargs": "^15.0.8",
		"eslint": "^7.11.0",
		"jest": "^26.0.0",
		"ts-jest": "^26.4.3",
